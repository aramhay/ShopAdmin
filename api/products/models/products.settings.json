    "video_description": {
      "type": "text"
    },
    "video_status": {
      "type": "string"
    },
    "video_name": {
      "type": "string"
    },
    "favorite_products": {
      "private": true,
      "collection": "favorite-product",
      "via": "product"
    },
    "approved_by_DPAB": {
      "type": "boolean"
