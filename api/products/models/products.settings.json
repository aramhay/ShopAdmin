    "brand": {
      "type": "string"
    },
    "for_sale_count": {
      "type": "biginteger"
    },
    "unit": {
      "type": "string"
    },
    "discount": {
      "type": "biginteger"
    },
    "type_test": {
      "private": true,
      "model": "type-test",
      "via": "product"
    },
    "category": {
      "private": true,
      "model": "category"
    },
    "sub_category": {
      "private": true,
      "model": "sub-category"
    },
    "menu_item": {
      "private": true,
      "model": "menu-item",
      "via": "products"
    },
    "name": {
      "type": "string"
    },
